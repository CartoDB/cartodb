{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie < 11"]
      },
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "plugins": [
    "@vue/babel-plugin-transform-vue-jsx",
    ["@babel/plugin-transform-runtime", { "corejs": 3 }],
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 1%",
                "last 2 versions",
                "not ie < 11"
              ]
            },
            "useBuiltIns": "usage",
            "corejs": 3
          }
        ]
      ],
      "plugins": [
        "@vue/babel-plugin-transform-vue-jsx",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
