<div class="Dialog-content Dialog-content--expanded">
  <div class="Dialog-contentWrapper">

    <div class="Dialog-header Dialog-header--expanded CreateDialog-header">
      <ul class="CreateDialog-headerSteps">
        <li class="CreateDialog-headerStep CreateDialog-headerStep--single">
          <div class="Dialog-headerIcon Dialog-headerIcon--neutral">
            <i class="CDB-IconFont CDB-IconFont-marker"></i>
          </div>
          <p class="Dialog-headerTitle">Georeference your dataset</p>
          <p class="Dialog-headerText">Transform your data into coordinates</p>
        </li>
      </ul>
    </div>

    <div class="Dialog-body Dialog-body--expanded Dialog-body--create">
      <div class="CreateDialog-listing <%- hasNoGeoferencedData ? 'CreateDialog-listing--withFlashMessage': '' %>">
        <% if (hasNoGeoferencedData) { %>
          <div class="FlashMessage FlashMessage--inDialogWithFiltersNavListing">
            <div class="u-inner">
              No georeferenced data on your layer. If you donâ€™t georeference your data your map will be blank.
            </div>
          </div>
        <% } %>
        <div class="Filters Filters--navListing <%- hasNoGeoferencedData ? 'Filters--navListing--withFlashMessage': '' %>">
          <div class="u-inner">
            <div class="Filters-inner">
              <div class="Filters-row Filters-row--centered">
                <span class="Filters-separator"></span>
                <ul class="Filters-group js-tabs"></ul>
              </div>
            </div>
          </div>
        </div>
        <div class="Dialog-bodyInnerExpandedWithPreFooter Georeference-content js-tab-content"></div>
      </div>
    </div>
  </div>
</div>
