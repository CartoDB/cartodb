    "jquery-cdb-v3": "global:$",
    "underscore-cdb-v3": "global:_",
    "cdb.admin": "global:cdb.admin",
    "cdb.Utils": "global:cdb.Utils",
    "cartodb.js-v3": "global:cdb",
    "backbone-cdb-v3": "global:Backbone",
    "moment-v3": "global:moment"
  },
