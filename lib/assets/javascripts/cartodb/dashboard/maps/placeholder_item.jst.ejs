<div class="MapCard">
  <div class="MapCard-fake"></div>
  <div class="MapCard-header MapCard-header--fake">
    <img src="<%- cdb.config.get('assets_url') %>/images/layout/map_templates/<%- icon %>.svg" />
  </div>
  <div class="MapCard-content">
    <div class="MapCard-contentBody">
      <div class="MapCard-contentBodyRow">
        <p class="MapCard-desc MapCard-desc--fake DefaultDescription CDB-Text CDB-Size-medium u-secondaryTextColor"><%- desc %></p>
      </div>
      <div class="MapCard-contentBodyRow MapCard-contentBodyRow--fake">
        <a href="<%- url %>" target="_blank">
          <button class="CDB-Button CDB-Button--secondary  js-open">
            <span class="CDB-Button-Text CDB-Text is-semibold CDB-Size-small u-upperCase">Learn more</span>
          </button>
        </a>
      </div>
    </div>
  </div>
</div>
