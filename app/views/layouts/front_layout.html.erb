<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <title>CartoDB | Login</title>
    <%= favicon_link_tag "/favicons/favicon.ico" %>
    <%= stylesheet_link_tag 'old_common.css', 'front.css', :media => 'all' %>
    <!--[if gt IE 8]><%= stylesheet_link_tag 'fonts_ie.css' %><![endif]-->
    <%= insert_trackjs() %>
    <%= insert_google_analytics('primary') %>
    <%= yield :js %>
    <noscript><meta http-equiv="refresh" content="0;url=/no_javascript.html"></noscript>
    <% if Rails.env.staging? %>
      <meta name="robots" content="noindex, nofollow">
    <% end %>
  </head>
  <body class="sessions">
    <%= yield %>
  </body>
</html>
