<header class="table">
  <div class="inner">
    <div class="left">
      <hgroup>
        <h1><%= link_to "Back", root_path %></h1>
        <h2 class="special"><%= link_to "", "#change_title", :class => "change_title" %></h2>
        <a class="clearview button_header rounded_white" href="#clearview">clear view</a>
        <a href="#change_status" class="status button_header rounded_white">.</a>
        <span class="tags"></span>
      </hgroup>
      <span class="table_description"><p></p></span>
      <nav>
        <%= link_to "Table",    "#/table", :class => "smaller strong tab selected" %>
        <%= link_to "Map view", "#/map",   :class => "smaller strong tab" %>
      </nav>
    </div>
    <ul class="right options">
      <li><%= link_to "options", "#options", :class => "dropdown options" %></li>
      <li><%= link_to current_user.username, "#" + current_user.username, :class => "dropdown account" %></li>
      <li><%= link_to "visualize", "#/map", :class => "rounded_white visualize" %></li>
    </ul>
    <div class="shadow"></div>
  </div>
  <div class="globalerror"></div>
</header>

