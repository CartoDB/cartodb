<ul>
  <li class="<% if (table.isInSQLView() && !dataLayer.get('query')) { %>disabled<% } %>"><a class="small export" href="#export">Export...</a></li>
  <% if (table.isInSQLView()) { %>
    <li class="<% if (table.isInSQLView() && !dataLayer.get('query')) { %>disabled<% } %>"><a class="small duplicate" href="#table_from_query">Table from query</a></li>
  <% } else { %>
    <li><a class="small georeference" href="#georeference">Georeference</a></li>
    <li><a class="small duplicate" href="#duplicate">Duplicate table...</a></li>
    <li><a class="small merge_tables" href="#merge_tables">Merge tables</a></li>
  <% } %>
  <li class="disabled"><a class="small append">Append data...</a></li>
  <li><a class="small delete_table red" href="#delete">Delete this table...</a></li>
</ul>
