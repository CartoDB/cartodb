<div class="Filters above-form">
  <div class="Filters-inner">
    <span class="Filters-separator"></span>
    <div class="Filters-row">
      <div class="Filters-type">
        <ul class="Filters-list">
          <li>
            <a href="<%- backUrl %>" class="Filters-actionButton NavButton NavButton--back">
              <i class="iconFont iconFont-ArrowPrev"></i>
            </a>
          </li>
          <li class="Filters-typeItem">
            <div class="FormAccount-title">
              <p class="FormAccount-titleText"><%- title %></p>
            </div>
          </li>
        </ul>
      </div>
      <div class="Filters-order">
        <ul class="Filters-list">
          <% if (usersUrl) { %>
            <li class="Filters-typeItem">
              <a href="<%- usersUrl %>" class="Filters-typeLink <%- usersUrl.isCurrent ? 'is-selected' : '' %>">
                <%- usersLabel %>
              </a>
            </li>
          <% } %>
          <li class="Filters-typeItem">
            <a href="<%- editUrl %>" class="Filters-typeLink <%- editUrl.isCurrent ? 'is-selected' : '' %>">
              Settings
            </a>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>

<div>
</div>
