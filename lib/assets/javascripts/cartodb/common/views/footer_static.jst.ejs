<footer id="Footer" class="CDB-Text CDB-FontSize-medium Footer <% if (light) { 'Footer--light' } %> Footer--public u-pr">
  <div class="CDB-Text CDB-FontSize-medium u-tSpace-xl Footer">
    <div class="u-inner Footer-inner">
      <ul class="Footer-list Footer-list--primary">
          <% if (!isHosted) { %>
            <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.guides') %></li>
            <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.developers') %></li>
          <% } %>
          <% if (onpremiseVersion && onpremiseVersion !== "") { %>
            <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.version', {onpremiseVersion: onpremiseVersion}) %></li>
          <% } %>
      </ul>

      <ul class="Footer-list Footer-list--secondary">
        <% if (onpremiseVersion && onpremiseVersion !== "") { %>
          <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.onpremise') %></li>
        <% } else { %>
          <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.support') %></li>
        <% } %>
        <li class="Footer-listItem CDB-Text CDB-Size-medium"><%= _t('cartodb.common.views.footer_static.contact') %></li>
      </ul>
    </div>
  </div>
</div>
