development:
  <<: *defaults
  http_port: 3000
  varnish_management:
    critical: false
    host: '127.0.0.1'
    port: 6082
    purge_command: 'purge'
    url_purge_command: 'url.purge'
    retries: 5
    timeout: 5
  enforce_non_empty_layer_css: false

