<div class="Dialog-header Dialog-header--expanded">
  <div class="Dialog-headerIcon Dialog-headerIcon--neutral">
    <i class="CDB-IconFont CDB-IconFont-marker"></i>
  </div>
  <p class="Dialog-headerTitle">Edit <%- featureType %> metadata</p>
  <p class="Dialog-headerText">
    Edit your <%- featureType %> data, or create new columns on your dataset.
  </p>
  <div class="FeatureData-mapWrapper">
    <div class="FeatureData-mapContainer js-map"></div>
    <div class="FeatureData-mapMamufas"></div>
  </div>
</div>
<div class="Dialog-expandedSubContent js-content">
  <div class="FeatureData-form">
    <form class="Form FeatureData-formContent js-form">
      <div class="Dialog-stickyFooter">
        <div class="Dialog-footer FeatureData-formFooter">
          <div><%/* placeholder for flex layout */%></div>
          <button type="submit" class="Button Button--main Button--inline js-submit">
            <span>Save</span>
          </button>
        </div>
      </div>
    </form>
  </div>
</div>
