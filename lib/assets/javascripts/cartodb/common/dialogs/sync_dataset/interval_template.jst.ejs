<div class="RadioButton js-interval<%- disabled ? ' is-disabled' : '' %><%- checked ? ' is-checked' : '' %><%- interval === 0 ? ' is-alert' : '' %>">
  <button class="RadioButton-input js-input<%- checked ? ' is-checked' : '' %>"></button>
  <label class="RadioButton-label"><%- name %></label>
</div>

