<ul>
  <% if (isPublic !== true && in_sql_view && !read_only) { %>
    <li class="tip"><a href="#/clear-view" class="clearview">Clear filters</a> to perform more actions</li>
  <% } %>
  <li class="order"><a class="small">Order</a> <div class="order_selector"><a class="asc" href="#asc">asc</a><a class="desc" href="#des">desc</a></div></li>
  <% if (reserved_column !== true && isPublic !== true && !in_sql_view) { %>
    <li><a class="small rename_column" href="#rename">Rename</a></li>
    <li class="alias">
      <div>
        <% if (columnAlias === null && columnAliasEdit === false) { %>
        <p>Column Alias:</p>
        <a class="alias_column" href="#alias_column">+ Add</a>
        <% } else if(columnAliasEdit){%>
        <p>Column Alias:</p>
        <span><input class="alias_column_input" id="aliasInput" type="text" value="<%- columnAlias %>" onfocus="this.value = this.value;"/><a class="save_column_alias" href="">Save</a></span>
        <% } else {%>
        <p>Column Alias:</p>
        <span><p><%- columnAlias %></p><a class="editAlias" href="">edit</a><a class="removeAlias">remove</a></span>
        <% } %>
      </div>
    </li>
    <li><a class="small change_data_type" href="#change_type">Change data type...</a></li>
    <li><a class="small georeference" href="#georeference">Georeference...</a></li>
    <li><a class="small add_new_column" href="#add_column">Add Column</a></li>
  <% } %>
  <% if (isPublic !== true) { %>
    <li><a class="small filter_by_this_column" href="#filter_by_column">Filter by this column</a></li>
  <% } %>
  <% if (reserved_column !== true && isPublic !== true && !in_sql_view) { %>
    <li><a class="small delete_column red" href="#delete_column">Delete</a></li>
  <% } %>
</ul>
