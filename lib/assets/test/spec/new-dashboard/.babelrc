{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
      }
    ]
  ],
  "plugins": [
    "@vue/babel-plugin-transform-vue-jsx",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ],
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-classes"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "useBuiltIns": "usage",
            "corejs": 3
          }
        ]
      ],
      "plugins": [
        "@vue/babel-plugin-transform-vue-jsx",
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3
          }
        ],
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-transform-classes"
      ]
    }
  }
}
