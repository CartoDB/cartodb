{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-object-assign",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "sourceType": "unambiguous",
  "env": {
    "test": {
      "presets": [
        ["@babel/env"],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-classes",
        "@babel/plugin-transform-object-assign",
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  }
}
