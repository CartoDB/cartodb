<div class="MapCard MapCard--selectable">
  <div class="MapCard-header MapCard-header--higher js-header">
    <div class="MapCard-loader"></div>
    <div class="MapCard-error"></div>
  </div>
  <div class="MapCard-content MapCard-content--compact">
    <div class="MapCard-contentBody">
      <div class="MapCard-contentBodyRow MapCard-contentBodyRow--flex">
        <h3 class="MapCard-title DefaultTitle u-ellipsLongText"><%- name %></h3>
      </div>
      <div class="MapCard-contentBodyRow MapCard-contentBodyRow--flex">
        <div class="MapCard-desc DefaultDescription"><%- description %></div>
      </div>
    </div>
    <div class="MapCard-contentFooter">
      <div class="MapCard-contentFooterDetails--left">
        <span class="DefaultTimeDiff"><%- timesUsed %> <%- timesUsedPluralize %> used</span>
      </div>
    </div>
  </div>
</div>